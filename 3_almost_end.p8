function upt_gift()
	for j=1,3 do
		for i in all(gift[j]) do
			i.x+=i.dx
			if i.x>=hand_r_x then
				deli(gift[j],1)
				kg+=1
			end
		end
	end
end

function drw_gift()
	for j=1,3 do
		for i in all(gift[j]) do
			thick_print(i.n,i.x,j2y(j)-1,i.n,7)
		end
	end
end

function add_gift()
	local g={x=0,n=flr(rnd(10)),dx=1}
	local j=ceil(rnd(3))
	add(gift[j],g)
end

function get_gift(k)
	del(gift[hand_j],gift[hand_j][k])
end

function back_gift(k)
	gift[hand_j][k].dx*=-1
end

function gift_in_hand(k)
	local x=gift[hand_j][1].x
	return x==mid(x,hand_l_x,hand_r_y)
end

function control_hand()
	if (btnp(üÖæÔ∏è)) hand_get=not hand_get
	clap,claped=btn(‚ùé),clap
	if clap and not claped then
		for i=1,#gift[hand_j] do
			if gift_in_hand(i) then
				if hand_get then
					get_gift(i)
				else
					back_gift(i)
				end
			end
		end
	end
end

function j2y(j)
	return (j-1)*14+61
end

function drw_play_background()
	fillp(‚ñí)
	circfill(-69,-91,265,5)
	fillp(‚ñë)
	for i=1,3 do
		line(9,j2y(i),63,j2y(i),0)
	end
	fillp()
end

sweat={
	add=function (self)
		local s={}
		s.d=rnd(hp/fat)
		s.t=tick+200-2*rnd(hp)
		s.x=94+rnd(10)-5
		s.y=51+rnd(10)-5
		s.h=3
		s.c=12
		add(self,s)
	end,
	upt=function(self)
		for i in all(self) do
			i.x+=i.d*cos((tick-i.t)/10*hp)
			i.y+=i.d*sin(tick/100*hp)
			if (i.t<tick) del(i,self)
			
		end
	end,
	drw=function(self)
		for i in all(self) do
			pset(i.x,i.y,i.c)
		end
	end
}
function move_hand()
	local prev_hand_j=hand_j
	local dir=0
	if (btnp(‚¨áÔ∏è)) dir=1
	if (btnp(‚¨ÜÔ∏è)) dir=-1
	if hp>0 and dir!=0 then
		hand_j=mid(1,hand_j+dir,3)
		if prev_hand_j!=hand_j then
			hp-=1
		end
	end
end

function add_order(t)
	local o={}
	local k=ceil(rnd(2))
	local tbl
	for i=1,k do
		add(tbl,flr(rnd(max_order))+1)
	end
	o.list=tbl
	o.t=t
	add(order,o)
end

function thick_print(str,x,y,c1,c2)
	for i in all({{-1,0},{1,0},{0,1},{0,-1}})	do
		print(str,x+i[1],y+i[2],c1)
	end
	print(str,x,y,c2)
end

function thick_str_chain(str,c1,c2)
	for i in all({{-1,0},{1,0},{0,1},{0,-1}})	do
		str_chain(str,.1,1+i[1],121+i[2],127,c1)
	end	
		str_chain(str,.1,1,121,127,c2)
end

function str_chain(str,spd,x,y,w,c)
	str..=' '
	clip(x,y,w,y+4)
	for i=1,w/4 do
		local char=str[(i+flr(tick*spd))%#str+1]
		print(char,x,y,c)
		x+=char>'‚ñà'and 8 or 4
	end
	clip()
end

function upt_tick()
	tick+=1
end

function clock()
	local str=''
	for i=93,94 do
		local s=tostr(stat(i))
		if (#s<2) s='0'..s
		if (i!=94) s=s..':'
		str..=s
	end
	return str
end

function drw_clock(str,x,y,c,blink)
	if stat(95)%2==0 
	and blink==true then
		str=sub(str,1,2)..' '..sub(str,4,5)
	end
	print(str,x,y,c)
end
