function drw_start(spd)
	cls(12)
	fillp(‚¨ÜÔ∏è)
	rectfill(0,0,127,127,11)
	fillp()
	str_chain("go to work with ‚ùé",-.4,21,35,86,7)
	str_chain(open_time,.15,21,53,26,8)
end

function drw_play()
	cls(2)
	drw_play_background()
	drw_info()
	drw_order()
	drw_clock(clock(),1,1,8,true)
	drw_body()
	drw_gift()
	thick_str_chain("hungry!",3,8)
end

function drw_info()
	local n
	for i=1,3 do
		local x,y=1,2+i*7
		if (i==1) n=‚òÖ
		if (i==2) n=hp
		if (i==3) n=fat
		rectfill(x,y,x+n,y+5,n)
		rect(x,y,x+11,y+5,2)
		rectfill(13,y,26,y+4,6)
	end
	color(10)
	print('‚òÖ',17,9)
	print('hp',17,16)
	print('fat',15,23)
end

function drw_order()
	fillp(‚ñ§)
	rectfill(28,1,125,31,2)
	fillp()
	for k,v in pairs(order) do
		local y=4
		local x=103-(k-1)*25
		rectfill(x+1,1,x+22,31,10)
		rectfill(x,2,x+21,32,11)
		for i in all(v.list) do
			thick_print(i,x+10,y,i,0)
			y+=8
			?'asdf',30,30,0
		end
		print(v.t,x+1,26)
	end
end

